<Window x:Class="project.AddSparePartsForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project"
        mc:Ignorable="d"
        Title="AddSparePartsForm" Height="450" Width="364">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="textBoxParts" Margin="0,0,0,10" Text="Введите запчасти, разделенные запятыми" />
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Content="Добавить" Click="Button_Add_Click" Margin="0,0,5,0"/>
                <Button Content="Отменить" Click="Button_Cancel_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
